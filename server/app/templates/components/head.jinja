<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, viewport-fit=cover">
    <meta http-equiv="X-UA-Compatible" content="ie=edge">
    <title>{{ branding.app_name }}</title>
    <meta name="msapplication-TileColor" content="#066fd1">
    <meta name="theme-color" content="#066fd1">
    <meta name="apple-mobile-web-app-status-bar-style" content="black-translucent">
    <meta name="apple-mobile-web-app-capable" content="yes">
    <meta name="mobile-web-app-capable" content="yes">
    <meta name="HandheldFriendly" content="True">
    <meta name="MobileOptimized" content="320">
    <link rel="icon" href="{{ branding.icon }}" type="image/x-icon">
    <link rel="shortcut icon" href="{{ branding.icon }}" type="image/x-icon">
    <meta name="description" content="{{ branding.app_description }}">
    <meta name="canonical" content="">
    <meta name="twitter:image:src" content="{{ branding.icon }}">
    <meta name="twitter:site" content="@tabler_ui">
    <meta name="twitter:card" content="summary">
    <meta name="twitter:title" content="{{ branding.app_name }}">
    <meta name="twitter:description" content="{{ branding.app_description }}">
    <meta property="og:image" content="{{ branding.icon }}">
    <meta property="og:image:width" content="1280">
    <meta property="og:image:height" content="640">
    <meta property="og:site_name" content="{{ branding.app_name }}">
    <meta property="og:type" content="object">
    <meta property="og:title" content="{{ branding.app_name }}">
    <meta property="og:url" content="{{ branding.app_url }}">
    <meta property="og:description" content="{{ branding.app_description }}">
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/@tabler/core@1.4.0/dist/css/tabler.min.css" />
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/@tabler/icons-webfont@latest/dist/tabler-icons.min.css" />
    <link rel="stylesheet" href="/static/css/branding.css" />
    <style>
        /* CSS Variables - Dynamic branding colors */
        :root {
            --brand-primary: {{ branding.primary_color }};
            --brand-secondary: {{ branding.secondary_color }};
            --brand-accent: {{ branding.accent_color }};
            --brand-primary-light: {{ branding.primary_color }}20;
            --brand-secondary-light: {{ branding.secondary_color }}20;
            --brand-accent-light: {{ branding.accent_color }}20;
        }
    </style>
</head>